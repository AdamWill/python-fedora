msgstr "Leyendo desde %s"

#: fedora/client/fas2.py:187
#, python-format
msgid "FAS server unable to retrieve group %s"
msgstr "El servidor FAS no pudo obtener el grupo %s"

#: fedora/client/fas2.py:299
msgid "key must be one of \"id\", \"username\", or \"email\""
msgstr "la llave debe ser uno de \"id\", \"username\" o \"email\""

#: fedora/client/fas2.py:305
#, python-format
msgid "%(field)s is not a valid field to filter"
msgstr "%(field)s no es un campo válido para filtrar"

#: fedora/client/fas2.py:354
msgid "people_by_id() is deprecated and will be removed in 0.4.  Please port your code to use people_by_key(key='id', fields=['human_name', 'email', 'username', 'bugzilla_email']) instead"
msgstr "people_by_id() está depreciado y será removido en 0.4. Por favor porte su código para usar people_by_key(key='id', fields=['human_name', 'email', 'username', 'bugzilla_email']) en su lugar"

#: fedora/client/fas2.py:507
msgid "FAS server unable to retrieve group members"
msgstr "El servidor FAS no pudo obtener los miembros del grupo"

#: fedora/client/fas2.py:529
msgid "FAS server unable to retrieve user information"
msgstr "El servidor FAS no pudo obtener la información del usuario"

#: fedora/client/pkgdb.py:188
#, python-format
